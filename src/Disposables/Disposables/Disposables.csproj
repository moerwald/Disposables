<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.1</Version>
    <Description>Simple object wrappers over the Dispose pattern.</Description>
    <PackageProjectUrl>https://github.com/moerwald/Disposables</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/moerwald/Disposables/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/moerwald/Disposables</RepositoryUrl>
    <PackageId>Moerwald.Disposables</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>

</Project>